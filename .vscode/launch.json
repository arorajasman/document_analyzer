  "version": "0.2.0",
  "configurations": [
    {
      "name": "document_analyzer",
      "request": "launch",
      "type": "dart"
    },
    {
      "name": "document_analyzer (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "document_analyzer (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    },

    {
      "name": "document_analyzer (web)",
      "request": "launch",
      "type": "dart",
      "program": "lib/main.dart",
      "args": ["-d", "chrome"]
    },
    {
      "name": "document_analyzer (web profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile",
      "program": "lib/main.dart",
      "args": ["-d", "chrome"]
    },
    {
      "name": "document_analyzer (web release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release",
      "program": "lib/main.dart",
      "args": ["-d", "chrome"]
    }
  ]
}
